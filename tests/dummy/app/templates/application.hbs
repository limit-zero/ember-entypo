<div class="container my-3">
  <div class="row">
    <div class="col">
      <h2 id="title">Ember Entypo Icons</h2>

      <table class="table table-striped">
        <thead>
          <tr>
            <th>Example</th>
            <th>Name</th>
            <th>Usage</th>
          </tr>
        </thead>
        <tbody>
          {{#each icons as |icon|}}
            <tr>
              <td style="font-size: 2rem;">{{entypo-icon icon}}</td>
              <td class="align-middle">entypo-{{icon}}</td>
              <td class="align-middle"><code>{{"{{entypo-icon "}} "{{icon}}"{{"}}"}}</code></td>
            </tr>
          {{/each}}
        </tbody>
      </table>

    </div>
  </div>
</div>




{{outlet}}
